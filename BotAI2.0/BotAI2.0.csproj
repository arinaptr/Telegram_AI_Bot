    <PackageReference Include="IronOcr.Languages.Russian" Version="2020.11.2" />
    <PackageReference Include="IronPython" Version="3.4.0" />
    <PackageReference Include="NAudio" Version="2.1.0" />
    <PackageReference Include="Telegram.Bot" Version="18.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="VoiceRSS_TTS_SDK">
      <HintPath>bin\Debug\net6.0\VoiceRSS_TTS_SDK.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
